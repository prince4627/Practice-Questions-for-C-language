#include <stdio.h>

void main() {
    int n,n1,n2=0;
    printf("Enter a number \n");
    scanf("%d",&n);
    n1=n;
    while (n>0)
    {
        n2*=10;
        n2 = n2+(n%10);
        n/=10;
    }
    if  (n1==n2)
        printf("The given number is palindrome");
    else 
        printf("The given number is not palindrome");
    
}

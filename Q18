#include <stdio.h>

void main() {
    int n,n1,n2,sum=0;
    printf("Enter a number \n");
    scanf("%d",&n);
    n1=n;
    while (n>0)
    {
        n2=n%10;
        sum += n2*n2*n2;
        n=n/10;
    }
    if  (n1==sum)
        printf("The given number is an armstrong number");
    else 
        printf("The given number is not an armstrong number");
    
}

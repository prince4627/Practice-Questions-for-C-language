#include <stdio.h>

void main() {
    
    int n=5;
    
    for (int i=1,k=1; i<=n; i++)
	{
	    for (int j=1; j<=i; j++)
	    {
	        
	        if (k%2==1)
	        {
                printf("%c ",96+k++);
	        }
	        else
	        {
	            printf("%c ",64+k++);
	        }
	    }
	    
	    printf("\n");
	}

}

/* Output
a 
B c 
D e F 
g H i J 
k L m N o 
*/
